---
nositemap: true
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:atom="http://www.w3.org/2005/Atom" xmlns:sy="http://purl.org/rss/1.0/modules/syndication/">
	<channel>
		<title>schaechinger</title>
		<atom:link href="{{ page.url | absolute_url }}" rel="self" type="application/rss+xml"/>
		<link>{{ site.url }}/blog</link>
		<lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
		<language>de-DE</language>
		<sy:updatePeriod>hourly</sy:updatePeriod>
		<sy:updateFrequency>1</sy:updateFrequency>
		<image>
			<url>{{ site.imageBase }}/2/schaechinger.jpg</url>
			<title>schaechinger.com</title>
			<link>{{ site.url }}/blog</link>
			<width>64</width>
			<height>64</height>
		</image>

		{% assign posts = site.posts %}
		{% for post in posts limit: 25 %}
			<item>
				<title>{{ post.title | smartify | strip_html | normalize_whitespace | xml_escape }}</title>
				<link>https://schchn.gr/{{ post.shortkey }}</link>
				<guid isPermaLink="true">https://schchn.gr/{{ post.shortkey }}</guid>
				<pubDate>{{ post.last_modified_at | default: post.date | date_to_rfc822 }}</pubDate>
				{% if post.category %}
					<category>{{ post.category | xml_escape }}</category>
				{% endif %}
				{% for tag in post.tags %}
					<category>{{ tag | xml_escape }}</category>
				{% endfor %}
				<dc:creator>{{ 'Manuel Sch√§chinger' | xml_escape }}</dc:creator>
				<description>{{ post.excerpt | strip | xml_escape }}</description>
			</item>
		{% endfor %}
	</channel>
</rss>
